               <TableColumn fx:id="dataColumn" prefWidth="100" text="Data" />
               <TableColumn fx:id="durataColumn" prefWidth="100" text="Durata" />
               <TableColumn fx:id="descrizioneColumn" prefWidth="200" text="Descrizione" />
