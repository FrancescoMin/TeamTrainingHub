ent.setString(3, String.valueOf(allenamento.getOrarioFine()));\r\n        statement.setString(3, allenamento.getDescrizione());\r\n        statement.setString(4, utenti_email);\r\n        return statement;\r\n    }\r\n\r\n}\r\n
